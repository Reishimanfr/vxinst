services:
  vxinstagram:
    build:
      context: .
      dockerfile: Dockerfile
    ports:
      - "8080:8080"
    environment:
      - PORT=8080
      - GIN_LOGS=false
      - SECURE=false
      - LOG_LEVEL=info
      - CERT_FILE=
      - KEY_FILE=
      - SENTRY_DSN=
    restart: unless-stopped
